// Usa switch para mostrar o nome do mês conforme o valor de "mes"
switch (mes) {
    case 1: // se mes = 1
        console.log("Janeiro");
        break; // para aqui, não continua pros outros

    case 2:
        console.log("Fevereiro");
        break;

    case 3:
        console.log("Março");
        break;

    case 4:
        console.log("Abril"); // se mes = 4
        break;

    case 5:
        console.log("Maio");
        break;

    case 6:
        console.log("Junho");
        break;

    case 7:
        console.log("Julho");
        break;

    case 8:
        console.log("Agosto");
        break;

    case 9:
        console.log("Setembro");
        break;

    case 10:
        console.log("Outubro");
        break;

    case 11:
        console.log("Novembro");
        break;

    case 12:
        console.log("Dezembro");
        break;

    default: // se não for nenhum dos acima
        console.log("Mês inválido");
}
